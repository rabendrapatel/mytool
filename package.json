{
  "name": "handy-tools",
  "version": "0.0.1",
  "description": "",
  "main": "./src/server/dist/handy/core/start-server.js",
  "scripts": {
    "dev": "concurrently -r \"npm run watch-ng\" \"nodemon --exec npm run dev-start\" \"npm run serve-build\"",
    "dev-12": "concurrently -r \"npm run watch-ng\" \"nodemon --exec npm run dev-start-12\" \"npm run serve-build\"",
    "dev-serve": "concurrently -r \"nodemon --exec npm run dev-start\" \"npm run serve-client\"",
    "dev-start": "cd src/server && rm -Rf dist && node ./handy/core/pre-run/pre-run.js && tsc -b && node ./dist/handy/core/start-server.js",
    "dev-start-12": "cd src/server && rm -Rf dist && node ./handy/core/pre-run/pre-run.js && tsc -b && node --tls-min-v1.0 ./dist/handy/core/start-server.js",
    "build-server": "cd src/server && rm -Rf dist && node ./handy/core/pre-run/pre-run.js && tsc -b",
    "watch-ng": "cd src/server && node ./handy/core/pre-run/ng-reload.js",
    "pre-run": "cd src/server && node ./handy/core/pre-run/pre-run.js",
    "serve-client": "cd src/client/web && ng serve",
    "serve-build": "cd src/client/web && ng build --watch",
    "build-client": "cd src/client/web && npm run build:ssr --prod",
    "build": "npm run build-server && npm run build-client",
    "install-all": "npm i --save && cd src/client/web && npm i --save",
    "server-12": "Node_ENV=production && node --tls-min-v1.0 ./src/server/dist/handy/core/start-server.js",
    "server": "Node_ENV=production && node ./src/server/dist/handy/core/start-server.js"
  },
